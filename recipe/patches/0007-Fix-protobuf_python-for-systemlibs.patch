         },
         urls = tf_mirror_urls("https://github.com/protocolbuffers/protobuf/archive/v3.21.9.zip"),
     )
@@ -872,10 +873,10 @@ def _tf_repositories():
 
     tf_http_archive(
         name = "pybind11_protobuf",
